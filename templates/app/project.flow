{
  luxe:{
    window: {
      width: <%= width %>,
      height: <%= height %>,
      title:'<%= name %>',
      fullscreen:false,
      resizable:true,
      borderless:false
    }
  },

  project : {
    name : '<%= name %>',
    version : '0.0.1',
    author : '<%= name %>',

    app : {
      name : 'luxe_empty',
      package : 'com.<%= author %>.<%= name %>'
    },

    build : {
      dependencies : {
        luxe : '*',
      }
    },

    files : {
      assets : 'assets/'
    }
  }
}
